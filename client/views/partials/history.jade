//Challenge history (limit 10, sorted ascending)
h5(ng-hide='history.length'): em History? Never heard of it.
.history(ng-repeat='point in history')
  .historyUsername Contender: {{point.user.username}}
  .historyDeckName Deck: {{point.deck.name}}
  .historyCategory Category: {{point.deck.category}}
  .historyWinner Winner: {{point.winner}}
  .historyDate Date: {{point.date | date}}